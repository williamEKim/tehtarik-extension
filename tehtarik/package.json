{
  "name": "tehtarik",
  "displayName": "TehTarik Language Support",
  "icon": "icon.png",
  "publisher": "wkim",
  "license": "MIT",
  "description": "Syntax highlighting for the TehTarik programming language.",
  "version": "1.1.2",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/williamEKim/tehtarik-extension.git"
  },
  "contributes": {
    "iconThemes": [
      {
        "id": "tehtarik-icons",
        "label": "TehTarik File Icons",
        "path": "./file-icons.json"
      }
    ],
    "languages": [
      {
        "id": "tehtarik",
        "aliases": [
          "TehTarik"
        ],
        "extensions": [
          ".tt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tehtarik",
        "scopeName": "source.tehtarik",
        "path": "./syntaxes/tehtarik.tmLanguage.json"
      }
    ]
  }
}
